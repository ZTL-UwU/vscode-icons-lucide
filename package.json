    "svg2ttf": "^6.0.3",
    "svgicons2svgfont": "^15.0.1",
    "ttf2woff": "^3.0.0"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@vscode/vsce-sign"
  ]
