{
  "publisher": "ztluwu",
  "name": "lucide-icons",
  "version": "0.3.0",
  "private": true,
  "type": "module",
  "engines": {
    "vscode": "*"
  },
  "displayName": "Lucide Product Icon Theme",
  "description": "Lucide Product Icon Theme for VS Code",
  "author": {
    "name": "Tony Zhang"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZTL-UwU/vscode-icons-lucide.git"
  },
  "bugs": {
    "url": "https://github.com/ZTL-UwU/vscode-icons-lucide/issues"
  },
  "keywords": [
    "icon",
    "theme",
    "product",
    "product-icon-theme"
  ],
  "categories": [
    "Themes"
  ],
  "icon": "icon.png",
  "extensionKind": [
    "ui"
  ],
  "sponsor": {
    "url": "https://ko-fi.com/ztl_uwu"
  },
  "contributes": {
    "productIconThemes": [
      {
        "id": "lucide-icons",
        "label": "Lucide Product Icon Theme",
        "path": "./dist/lucide-icons.json"
      }
    ]
  },
  "files": [
    "dist/lucide-icons.{json,woff}",
    "LICENSE",
    "icon.png"
  ],
  "scripts": {
    "build": "bun scripts/build.ts",
    "lint": "biome lint",
